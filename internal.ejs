</head>
<body style="background-image: url(C:\Users\Mubarak\Tiffian\views\Bg.png);">
	<div class="ass">
        <% data.forEach(function(dataname){ %>
            <li class="qwe" style="border: 3px solid orange">
              <td class="a1"><h2><%= dataname.name%></h2></td>
              <td class="a2"><h4><%= dataname.desc%></h4></td>
              <td class="a3"><h4><%= dataname.price%></h4></td>
              
            </li>
         
         <% }) %>
         </ul>
    </div>
